
.container {
  background-color: #f5f6f8;
}
.header-view {
  display: flex; 
padding: 30rpx;

}
.date-view {
  width: 270rpx;
  display: flex;flex-direction: row;align-items: center;
  float: right;
  margin-left: 340rpx;
}
.left-icon {
  width: 50rpx;height: 50rpx;
}

.date-icon {
  width: 50rpx;
  height: 50rpx;
  margin-right: 10rpx;
}


.food-info {
  background-color: #ffffff;
  padding: 60rpx;margin-top: 20rpx;
}
.mune-order {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}

.order_btn {
  width: 90%;
  margin: 50rpx auto 0;
  background-color: #902bc3;
  color: #ffffff;
}

.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 50rpx auto 0;
  background-color: #ffffff;
  padding: 30rpx 0;
}

.logo {
  width: 50%;
}

.userinfo-nickname {
  font-size: 16px;
  line-height: 70rpx;
  color: #646464;
}